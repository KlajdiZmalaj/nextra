html:root {
  --accent: #ffeb3b;
  --accent-bg: #ffeb3b;
  --accent-txt: #222;
  --primary-bg: #172a5c;
  --primary-txt: #fff;
  --secondary-bg: #081538;
  --secondary-txt: #fff;
  --mainBg: #020c22;
  --mainBgCol: #fff;
  --bg1: #172a5c;
  --col1: #fff;
  --teableHeadersBg: #102140;
  --teableHeadersCol: #ffffff;
  --teableSubHeadersBg: #081732;
  --teableSubHeadersCol: #ffffff;
  --tableBorders: #47598a;
  --casinoGradient1: #00000000;
  --casinoGradient1-txt: rgba(255, 255, 255, 0);
  --casinoGradient2: #636363;
  --casinoGradient2-txt: #000;
  --backDrops: #000000cf;
