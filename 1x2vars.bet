$secondary: #ffeb3b;
$secondaryCol: #fff;
$black: #000000;
$secondaryDark: #ffeb3b;
$secondaryDarkCol: #fff;
$primary: #172a5d;
$white: #ebebeb;
$blue: #262ca8;
$green: #2e9c3d;
$red: #e1042e;
$sb-2: #172a5d;
body {
  background-size: 2038px !important;
}
#favouriteTournaments li:before {
  filter: brightness(0);
}
body.landing-page {
  background-color: #0e0c0d !important;
  background-image: none;
}

.main_header .main_header_left {
  width: 200px !important;
}

.casino_container .casino_type .ct_games_container > .ctg_games:nth-child(1) {
  &:before {
    background-image: url(//nextra.pcluster.info/1x2sportbet/images/casinolive/clive-banner1.jpg) !important;
  }
}



//make changes

.fas.fa-eye {
  cursor: pointer;
}

.header-buttons.hide {
display: none !important;
}


.buttons-holder a.single-button:nth-child(2) {
font-size: 0px !important;
}

.buttons-holder a.single-button i.fa.fa-print {
  font-size: 21px;
font-weight: 300 !important;
}



.logged-content.admin-content .row  .buttons-holder .single-button:nth-child(2)::after {
  content: 'Stampa Quota';
  font-size: 21px;
  font-weight: bold;
  display: inline-block;
}

.widget-wrapper ul#favouriteSports {
  background: #23221e;
}

body .center-content .percentage-block .single-over-under .team-names {
  width: 90%;
  margin: auto;
}


.explore-matches-page .title {
  border-bottom: 1px solid #6d6d6d;
  line-height: 36px;
  width: calc(100% - 70px);
  padding: 25px;
  display: inline-block;
}